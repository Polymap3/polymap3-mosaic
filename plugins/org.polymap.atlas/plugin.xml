<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="org.polymap.atlas.applicationLayouters" name="Atlas application layouters" schema="schema/applicationLayouters.exsd"/>
   <extension
         point="org.eclipse.rap.ui.entrypoint">
      <entrypoint
            class="org.polymap.atlas.AtlasApplication"
            id="org.polymap.atlas.entrypoint"
            path="/atlas">
      </entrypoint>
   </extension>
   <extension
         point="org.polymap.atlas.applicationLayouters">
      <applicationLayouter
            class="org.polymap.atlas.internal.desktop.DesktopApplicationManager"
            id="org.polymap.atlas.desktopApplicationLayouter">
      </applicationLayouter>
   </extension>

</plugin>
